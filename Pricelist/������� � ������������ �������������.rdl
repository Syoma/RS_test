  <DataSets>
    <DataSet Name="uncat">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT
       pricelist_set.name    plist_name,
       p_col.name               p_col_name,
       p_row.name               p_row_name,
       pricelist_loop.discount  discount,
       psplink.ord sordorder
  FROM
       psplink
       join pricelist_set on pricelist_set.id = psplink.plset
       join pricelist p_col on p_col.id = psplink.plist
       join pricelist_loop on pricelist_loop.parent = psplink.plist
       join pricelist p_row on p_row.id = pricelist_loop.id
 WHERE
       psplink.plset = ?
UNION ALL
SELECT
       ''    plist_name,
       'CC'               p_col_name,
       p_row.name               p_row_name,
       -costrule.discount*100  discount,
       -1 sordorder
  FROM costrule, pricelist p_row
WHERE costrule.plist = p_row.id 
AND today() between costrule.ddateb and costrule.ddatee
ORDER BY sordorder</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter1.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>unact_my</DataSourceName>
      </Query>
      <Fields>
        <Field Name="plist_name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>plist_name</DataField>
        </Field>
        <Field Name="p_col_name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>p_col_name</DataField>
        </Field>
        <Field Name="p_row_name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>p_row_name</DataField>
        </Field>
        <Field Name="discount">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>discount</DataField>
        </Field>
        <Field Name="sordorder">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>sordorder</DataField>
        </Field>
      </Fields>
    </DataSet>
