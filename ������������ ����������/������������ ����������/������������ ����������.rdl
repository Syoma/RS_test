      <Query>
        <DataSourceName>rc_unact_old</DataSourceName>
        <CommandText>SELECT 0 
                as id,
                'Все' as name
UNION ALL
SELECT
	id,
	name
FROM
	pps_zone pz
WHERE
	pz.name like '%(инк)'</CommandText>
