    <DataSet Name="DataSource1">
      <Fields>
        <Field Name="Количество_созданных_задач">
          <DataField>Количество созданных задач</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Дата">
          <DataField>Дата</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Сервис">
          <DataField>Сервис</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Источник">
          <DataField>Источник</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select count(j.pkey) as [Количество созданных задач], CAST(j.CREATED as date) as [Дата], cv2.stringvalue as [Сервис], cv.STRINGVALUE as [Источник] from [JIRA].[jira].[jiraissue] j
join jira.jira.customfieldvalue cv on cv.ISSUE=j.id
join jira.jira.customfieldvalue cv2 on cv2.ISSUE=j.id
where cv.CUSTOMFIELD='10790' and cv2.CUSTOMFIELD='10420'
and j.created between @tn and @tk
group by CAST(j.CREATED as date), j.REPORTER, cv2.STRINGVALUE, cv.STRINGVALUE
order by CAST(j.CREATED as date), j.REPORTER, cv2.STRINGVALUE, cv.STRINGVALUE</CommandText>
        <QueryParameters>
          <QueryParameter Name="@tn">
            <Value>=Parameters!tn.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@tk">
            <Value>=Parameters!tk.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
