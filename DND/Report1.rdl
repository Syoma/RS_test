        <CommandText>create table #v1
(time datetime,
ev varchar (255),
ag varchar (255),
id int)
insert into #v1 (time, ev, ag, id)
select CONVERT(DATETIME, CONVERT(CHAR(19), time, 121)), event, agent, row_number() over (order by agent, time) from dbo.queue_log
where event ='addmember' and agent in (select distinct agent from dbo.queue_log where queuename='support_pps' and agent not in (select distinct agent from dbo.queue_log where queuename='it-support' or queuename='support_SUB_line2'  or queuename='support_SUB')) and CONVERT(DATE, CONVERT(CHAR(19), time)) between @ptime1 and @ptime2
order by agent, CONVERT(DATETIME, CONVERT(CHAR(19), time, 121))

create table #v2  
(time datetime,
id1 int,
ev1 varchar (255))
insert into #v2 (time, id1, ev1)
select CONVERT(DATETIME, CONVERT(CHAR(19), time, 121)), row_number() over (order by agent,time), event from dbo.queue_log
where event ='removemember' and agent in (select distinct agent from dbo.queue_log where queuename='support_pps' and agent not in (select distinct agent from dbo.queue_log where queuename='it-support' or queuename='support_SUB_line2' or queuename='support_SUB')) and CONVERT(DATE, CONVERT(CHAR(19), time)) between @ptime1 and @ptime2
order by agent, CONVERT(DATETIME, CONVERT(CHAR(19), time, 121))

create table #v3
(ev varchar (255),
ag varchar (255),
tn datetime, 
ev1 varchar (255),
tk datetime)
insert into #v3 (ev, ag, tn, ev1, tk)
select #v1.ev, #v1.ag, #v1.time, #v2.ev1, #v2.time from #v1, #v2
where #v1.id=#v2.id1 and CONVERT(DATETIME, CONVERT(CHAR(10), #v1.time, 121)) =CONVERT(DATETIME, CONVERT(CHAR(10), #v2.time, 121))

create table #v4
(time datetime,
ev varchar (255),
ag varchar (255),
id varchar (255))
insert into #v4 (time, ev, ag, id)
select CONVERT(DATETIME, CONVERT(CHAR(19), time, 121)), event, agent, callid from dbo.queue_log
where event ='connect' and agent in (select distinct agent from dbo.queue_log where queuename='support_pps' and agent not in (select distinct agent from dbo.queue_log where queuename='it-support' or queuename='support_SUB_line2' or queuename='support_SUB')) and CONVERT(DATE, CONVERT(CHAR(19),  time)) between @ptime1 and @ptime2
order by agent, CONVERT(DATETIME, CONVERT(CHAR(19), time, 121))

create table #v5 
(time datetime,
id1 varchar(255))
insert into #v5 (time, id1)
select CONVERT(DATETIME, CONVERT(CHAR(19), time, 121)), callid from dbo.queue_log
where event in ('completeagent','completecaller') and agent in (select distinct agent from dbo.queue_log where queuename='support_pps' and agent not in (select distinct agent from dbo.queue_log where queuename='it-support' or queuename='support_SUB_line2' or queuename='support_SUB')) and CONVERT(DATE, CONVERT(CHAR(19), time)) between @ptime1 and @ptime2
order by agent, CONVERT(DATETIME, CONVERT(CHAR(19), time, 121))

create table #v6
(ev varchar (255),
ag varchar (255),
tn datetime,
tk datetime)
insert into #v6 (ev, ag, tn, tk)
select #v4.ev, #v4.ag, #v4.time, #v5.time from #v4, #v5
where #v4.id=#v5.id1

create table #v7
(ag varchar (255),
tdnd datetime)
insert into #v7 (ag, tdnd)
select  distinct #v3.ag, convert (DATETIME, CONVERT(CHAR(19), dbo.queue_log.time, 121)) from dbo.queue_log, #v3
where event='ringnoanswer' and queuename='support_PPS' and CONVERT(DATE, CONVERT(CHAR(19), time)) between @ptime1 and @ptime2
and CONVERT(DATETIME, CONVERT(CHAR(19), dbo.queue_log.time, 121)) between #v3.tn and #v3.tk
except (select  distinct #v6.ag, convert (DATETIME, CONVERT(CHAR(19), dbo.queue_log.time, 121)) from dbo.queue_log, #v6
where event='ringnoanswer' and queuename='support_PPS' and CONVERT(DATE, CONVERT(CHAR(19), time)) between @ptime1 and @ptime2
and (CONVERT(DATETIME, CONVERT(CHAR(19), dbo.queue_log.time, 121)) between #v6.tn and #v6.tk)
)
create table #v8
(ag varchar (255),
tdnd datetime,
id int)
insert into #v8 (ag, tdnd, id)
select ag, tdnd, row_number() over (order by ag, tdnd)from #v7

create table #v9
(ag varchar (255), 
tdnd datetime)
insert into #v9 (ag, tdnd) select a.ag, a.tdnd from #v8 a,#v8 b, #v8 e
where a.id=b.id+1 and a.id=e.id-1 and DATEDIFF(second,b.tdnd, a.tdnd)&lt;30 and DATEDIFF(second,a.tdnd, e.tdnd)&lt;30 and a.ag=b.ag and a.ag=e.ag


select ag, cast (count(tdnd)*5.5/60 as decimal (10, 2) ) from #v9
group by ag</CommandText>
