      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>CREATE TABLE #tmp_terminal
    (
        id INTEGER,
        terminalid INTEGER,
        code VARCHAR(100),
        src_system INTEGER
    )
    INSERT INTO #tmp_terminal (id, terminalid, code, src_system)
    SELECT 
        id,
        terminalid,
        code,
        src_system
    FROM pps_terminal 
    WHERE pps_terminal.terminalid=?
SELECT top 3	
    list(ci.aname) aname, 
	ddates.ddate,
    pt.code code,
    list(info) info,
    cast(sum(isnull(ci.summ, 0)) as integer) cisumm,
    pi.summ ppssumm
FROM
    ddates 
        CROSS JOIN #tmp_terminal pt
        LEFT OUTER JOIN cat_incassation ci ON ci.terminalid=pt.terminalid and ci.ddate=ddates.ddate and ci.src_system=pt.src_system
        LEFT OUTER JOIN 
            (
                SELECT
                    pps_incassation.ddate,
                    pps_incassation.terminalid,
                    pps_incassation.src_system,
                    sum(pps_incassation.summ) summ
                FROM
                    pps_incassation 
                        JOIN #tmp_terminal ON #tmp_terminal.terminalid = pps_incassation.terminalid AND #tmp_terminal.src_system = pps_incassation.src_system
                WHERE
                    pps_incassation.ddate BETWEEN '2010-04-01' and getdate() + 1
                GROUP BY
                    pps_incassation.ddate,
                    pps_incassation.terminalid,
                    pps_incassation.src_system
            ) pi ON pi.terminalid=pt.terminalid and pi.src_system=pt.src_system and pi.ddate=ddates.ddate
WHERE 
    ddates.ddate BETWEEN '2010-04-01' and getdate() + 1
GROUP BY
    ddates.ddate,
    pt.code,
    pi.summ
    HAVING (count(ci.terminalid)&gt;0 OR pi.summ&gt;0)
ORDER BY
    pt.code,
    ddates.ddate desc</CommandText>
        <QueryParameters>
