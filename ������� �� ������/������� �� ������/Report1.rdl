      <Fields>
        <Field Name="Покупатель">
          <DataField>Покупатель</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Начислено">
          <DataField>Начислено</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Остаток">
          <DataField>Остаток</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
b.name as [Покупатель], 
sum(summ) as [Начислено], 
sum(rest_summ) as [Остаток] 
from dbo.NBVirtPurse nvp 
join NBAction na on na.id = nvp.nbaction
join buyers b on b.id = nvp.buyer
join partners p on p.id=b.partner
where p.id=?
and  dateadd(day,-datepart(day, ddateb)+1,CONVERT(date, ddateb)) &gt;= dateadd(day,-datepart(day, ?)+1,CONVERT(date, ?))
group by b.name</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter2.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter3.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
