  <DataSets>
    <DataSet Name="recept">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select
cd.name cdesk_name, 
cc.name client_name, 
CONVERT(VARCHAR(2),DAY(cr.ddate))+'-'+
(CASE 
WHEN MONTH(cr.ddate)&lt; 10 THEN '0'
END)+
CONVERT(VARCHAR(2),MONTH(cr.ddate))+'-'+
CONVERT(VARCHAR(4),YEAR(cr.ddate)) ddate

from cassa_recept cr
join cashdesks cd on cd.id=cr.cdesk
join cashclients cc on cc.id = cr.client
join partners p on p.id=cc.partner
join partners_sp_strings pss on pss.partner=p.id
where pss.sp_tp = (select id from sp_types where name = 'Код осмп')
and pss.value is not null
and cr.cash_class&lt;&gt;2 or cr.cash_class is null
or p.id in (select partner from partners_sp_sets where sp_tp = (select id from sp_types where name = 'К-партнер') and spv_id in (select id from sp_values where name = 'да'))
order by cr.ddate, cd.name, cc.name</CommandText>
        <DataSourceName>DataSource1</DataSourceName>
      </Query>
      <Fields>
        <Field Name="cdesk_name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>cdesk_name</DataField>
        </Field>
        <Field Name="client_name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>client_name</DataField>
        </Field>
        <Field Name="ddate">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>ddate</DataField>
        </Field>
      </Fields>
    </DataSet>
