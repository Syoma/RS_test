  <DataSets>
    <DataSet Name="UNCAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select 
d.ddate,
shortened,
( select min(ddatetime) from entrance where person_id = p.person_id and dir &gt; 0 and ddate = d.ddate ) ind,
( select max(ddatetime) from entrance where  person_id = p.person_id and dir &lt; 0 and ddate = d.ddate )  outd
from 
person p,
ddates d
where exists(
select 1 from emp_rel join emp_dept join emp_user_dept where ddatee is null and iam = user_id(current user) and p.person_id = person_id 
) and d.ddate between ? and  ?
order by
d.ddate,
shortened</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter2.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>UNCAT</DataSourceName>
      </Query>
