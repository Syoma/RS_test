    <DataSet Name="Mobile_Unekt">
      <Fields>
        <Field Name="fName">
          <DataField>fName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="category">
          <DataField>category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="cnt">
          <DataField>cnt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Mobile_Unekt</DataSourceName>
        <CommandText>select frep.fName, category, count(1) cnt
from
(select leg.sales_rep_fid, exid, category, x, y, count(1)
from 
(select
  sales_rep_fid,
  case when x1 &lt; x2 then x1 else x2 end x1,
  case when x1 &lt; x2 then y1 else y2 end y1,
  case when x1 &lt; x2 then x2 else x1 end x2,
  case when x1 &lt; x2 then y2 else y1 end y2
from 
(select t1.id sales_rep_fid, t1.Lon x1, t1.Lat y1, t2.Lon x2, t2.Lat y2
from DSGIS_Region t1
  join DSGIS_Region t2 on (
    t2.factiveflag = 1 and t2.id = t1.id
    and (t2.RecordId = t1.RecordId + 1
      or (t1.RecordId = (select max(t.RecordId) from DSGIS_Region t where t.id = t1.id and t.factiveflag = 1)
        and t2.RecordId = (select min(t.RecordId) from DSGIS_Region t where t.id = t2.id and t.factiveflag = 1)
      )
    )
  )
where t1.factiveflag = 1
) as leg_unordered (sales_rep_fid, x1, y1, x2, y2)
) as leg (sales_rep_fid, x1, y1, x2, y2)

join

(select cast(f.exid as int), c.category, cast(ax.AttrText as float) x, cast(ay.AttrText as float) y
from 
DS_Faces f 
  join DS_FacesAttributes ax on ax.fid = f.fid and ax.AttrID = 361
  join DS_FacesAttributes ay on ay.fid = f.fid and ay.AttrID = 360
  left outer join census..census_ba_raw_lym c on c.id = cast(f.exid as int)
where f.fType = 1 
  --and f.fid = 21848
) as point (exid, category, x, y)

on (
  x2 &gt; x
  and (
    (x1 = x2 and ((y1 &lt; y and y2 &gt; y) or (y1 &gt; y and y2 &lt; y)))
    or
    (((y1 &lt; y and y2 &gt; y) or (y1 &gt; y and y2 &lt; y)) and x &lt; x1 + ((y-y1)*(x2-x1))/(y2-y1))
  )
)
group by leg.sales_rep_fid, exid, category, x, y
having count(1)%2 = 1
) as terr_point (sales_rep_fid, exid, category, x, y, cnt)
  join DS_Faces frep on frep.fid = sales_rep_fid
group by frep.fName, category
/*having category &lt;&gt; 'x' and category &lt;&gt; 'закрылся'*/

order by 1, 2</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
