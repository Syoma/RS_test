          <rd:TypeName>System.String</rd:TypeName>
          <DataField>ТП</DataField>
        </Field>
        <Field Name="партнер">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>партнер</DataField>
        </Field>
        <Field Name="price">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>price</DataField>
        </Field>
