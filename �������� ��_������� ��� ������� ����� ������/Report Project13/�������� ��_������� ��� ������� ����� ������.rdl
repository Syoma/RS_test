  <DataSets>
    <DataSet Name="rc_unact">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select   
pg.name [ТП],
    p.name [партнер], 
    (
        SELECT 
            list(ps.name) 
        FROM 
            partners_plset ppl 
    join pricelist_set ps on ps.id=ppl.id
        WHERE 
            NOT EXISTS 
                (SELECT 1 FROM partner_plist WHERE partner_plist.partner = p.id AND ppl.id = partner_plist.plset)
and ppl.id not in (2021,6358,4619,1358,3001,1147,3222,1085,1008)
    ) price
from partners p
            join partners_groups pg on pg.id=p.parent
            left outer join partner_plist pp on pp.partner=p.id
            where  exists (select * from partners_groups_tree where id = p.parent and parent = 9093) 
            and pg.name not like '%Архив%'
           --and p.name = '777 ТД ООО Дм'

GROUP BY 
   p.id, p.name,pg.name 
having length(price)&gt;1
order by pg.name,p.name</CommandText>
        <DataSourceName>rc_unact</DataSourceName>
      </Query>
      <Fields>
