  <DataSets>
    <DataSet Name="prices">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact_old</DataSourceName>
        <CommandText>select id, name
from dbo.pricelist
order by name asc;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="currencies">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact_old</DataSourceName>
        <CommandText>select id, name
from dbo.currencies
order by parent asc, name asc;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="goods_groups">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="g_name">
          <DataField>g_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact_old</DataSourceName>
        <CommandText>select id, (repeat('  ', tlev) + name ) g_name
from goods_groups
order by sortcolumn asc;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="specifications">
      <Fields>
        <Field Name="st_id">
          <DataField>st_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="st_name">
          <DataField>st_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact_old</DataSourceName>
        <CommandText>select st.id st_id, st.name st_name
from sp_types st
join gg_sp_set on gg_sp_set.sp_tp = st.id
where gg_sp_set.g_group=?</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!g_group.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="spec_values">
      <Fields>
        <Field Name="sv_id">
          <DataField>sv_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="sv_name">
          <DataField>sv_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact_old</DataSourceName>
        <CommandText>select t.sv_id sv_id, t.sv_name sv_name
from (select 0 sv_id, '&lt;Не важно&gt;' sv_name, '###' sv_order from dummy
         union all
         select sv.id sv_id, sv.name sv_name, sv.name sv_order
         from sp_values sv
         where sp_tp=?
              and name &lt;&gt; '') t
order by t.sv_order asc</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!spec.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="remains_list">
      <Fields>
        <Field Name="r_id">
          <DataField>r_id</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="r_name">
          <DataField>r_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact_old</DataSourceName>
        <CommandText>select r_id, r_name
from (select 1 r_id, 'С учётом остатков' r_name from dummy
union all
select 0 r_id, 'Без учёта остатков' r_name from dummy) r</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="storages_groups">
      <Fields>
        <Field Name="sg_id">
          <DataField>sg_id</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="sg_name">
          <DataField>sg_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact_old</DataSourceName>
        <CommandText>select sg.id sg_id, (repeat('  ', sg.tlev) + sg.name ) sg_name
from storages_groups sg
order by sg.sortcolumn asc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="storage">
      <Fields>
        <Field Name="s_id">
          <DataField>s_id</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="s_name">
          <DataField>s_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact_old</DataSourceName>
        <CommandText>select t.s_id s_id, t.s_name s_name
from (select 0 s_id, '&lt;Все склады&gt;' s_name, '###' s_order
union all
select s.id as s_id, s.name as s_name, s.name s_order
from sa_split_list(?, ' ') s_groups
    join my_storagest on my_storagest.parent =  s_groups.row_value
    join storages s on s.id =  my_storagest.id
where my_storagest.iam= user_id(current_user)
) t
order by t.s_order asc</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=join(Parameters!s_group.Value)</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="main">
      <Fields>
        <Field Name="group_name">
          <DataField>group_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="goods_name">
          <DataField>goods_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sortcolumn">
          <DataField>sortcolumn</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="price">
          <DataField>price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact_old</DataSourceName>
        <CommandText>exec dbo.rep_goods_prices ?,?,?,?,?,?,?,?,?</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!pricelist.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!ddate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!curr.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!g_group.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!spec.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!spec_v.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!remains.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=join(Parameters!s_group.Value)</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=join(Parameters!store.Value)</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
  <Code>Shared Function SetColumn(ByVal row As Integer, ByVal column As Integer) As String
  While row &gt; column
    If column &gt;= row Then Exit While
    row = row - column
  End While

  Return row
End Function

Shared Function SetRow(ByVal row As Integer, ByVal column As Integer) As String
  Dim i As Integer = 1

  While row &gt; column
    i = i + 1
    If column &gt;= row Then Exit While
    row = row - column
  End While

  Return i
End Function</Code>
