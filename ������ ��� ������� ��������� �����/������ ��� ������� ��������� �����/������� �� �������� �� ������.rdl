          <DataField>ssum</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>cat</DataSourceName>
        <CommandText>exec ask_logistrent_keeping ?, ?, ?, ?, ?</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!ddateb.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!ddatee.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Join(Parameters!goods_groups.Value, ",")</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Join(Parameters!storages.Value,",")</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!korinpal.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="goods_groups">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>cat</DataSourceName>
        <CommandText>select
		goods_groups.id id,
		replicate('  ',2 * (goods_groups.tlev - (select gg.tlev from goods_groups gg where gg.id = 4087))) + goods_groups.name name
from
		goods_groups_tree ggt
			join goods_groups on goods_groups.id = ggt.id
where
		ggt.parent = 4087
order by
		goods_groups.sortcolumn</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="storages">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>cat</DataSourceName>
        <CommandText>select 
		id, 
		name 
from 
		storages 
where 
		storages.s_group in (select id from storages_groups_tree where parent =  (select id from storages_groups where name  = 'Логистические услуги'))
order by 
		name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="korinpal">
      <Fields>
        <Field Name="korinpal">
          <DataField>korinpal</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>cat</DataSourceName>
        <CommandText>select 
		row_num korinpal
from 
		rowgenerator 
where 
		row_num &lt; 201 
order by 
		row_num</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
