  <rd:ReportID>bc2c8e85-4671-4f1a-ab63-bec0fe141da8</rd:ReportID>
  <DataSets>
    <DataSet Name="subject">
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="code">
          <DataField>code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>proxycat</DataSourceName>
        <CommandText>select name, code
from
( select convert(varchar(255), '&lt;Все предметы&gt;' ) name, '' code
union
 select distinct oss.name name, oss.name code
 from rc_unact.rc_unact.dbo.osmp_sync_subject oss
) temp_oss
order by name asc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="result">
      <Fields>
        <Field Name="subject_name">
          <DataField>subject_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="src_system_name">
          <DataField>src_system_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="result_date">
          <DataField>result_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="result_status">
          <DataField>result_status</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="result_message">
          <DataField>result_message</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>proxycat</DataSourceName>
        <CommandText>select oss.name subject_name, isnull(ss.name, '&lt;Не задана&gt;') src_system_name, osr.ddate result_date, osr.status_int result_status, osr.status_message result_message
from RC_UNACT.rc_unact.dbo.osmp_sync_result osr
     join RC_UNACT.rc_unact.dbo.osmp_sync_subject oss on oss.id = osr.subject
    left outer join RC_UNACT.rc_unact.dbo.src_system ss on ss.id = oss.src_system
where (@is_error = 0 or osr.status_int &lt; 0)
	and (@src_system = 0 or oss.src_system = @src_system)
	and (@subject = '' or @subject=oss.name )
	and datediff(day, osr.ddate, @ddate_from) &lt;= 0 and datediff(day, osr.ddate, @ddate_to) &gt;= 0
order by oss.name asc, ss.name asc, osr.ddate asc</CommandText>
        <QueryParameters>
          <QueryParameter Name="@is_error">
            <Value>=Parameters!is_error.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@src_system">
            <Value>=Parameters!src_system.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@subject">
            <Value>=Parameters!subject.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ddate_from">
            <Value>=Parameters!ddate_from.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ddate_to">
            <Value>=Parameters!ddate_to.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
  <Code>Shared Function SetColumn(ByVal row As Integer, ByVal column As Integer) As String
  While row &gt; column
    If column &gt;= row Then Exit While
    row = row - column
  End While

  Return row
End Function

Shared Function SetRow(ByVal row As Integer, ByVal column As Integer) As String
  Dim i As Integer = 1

  While row &gt; column
    i = i + 1
    If column &gt;= row Then Exit While
    row = row - column
  End While

  Return i
End Function</Code>
