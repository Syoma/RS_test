  <DataSets>
    <DataSet Name="manager">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact</DataSourceName>
        <CommandText>select
		agents.id   id,
		agents.name name
from 
		agents
			join workpos on workpos.id = agents.position
where 
		workpos.name = 'Менеджер ОСМП'
UNION ALL
SELECT 0, '_Все'
ORDER BY 2</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="data_main">
      <Fields>
        <Field Name="manager_name">
          <DataField>manager_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="system_name">
          <DataField>system_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dealer_name">
          <DataField>dealer_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="subdealer_name">
          <DataField>subdealer_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="terminalid">
          <DataField>terminalid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="terminalcode">
          <DataField>terminalcode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="terminaladdress">
          <DataField>terminaladdress</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ddateb">
          <DataField>ddateb</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact</DataSourceName>
        <CommandText>SELECT 
    a.name manager_name,
    ss.name system_name,
    d.name dealer_name,
    s.name subdealer_name,
    pt.terminalid terminalid,
    pt.code terminalcode,
    pt.address terminaladdress,
    pt.ddateb
FROM 
    dealer d
        JOIN pps_terminal pt ON pt.branch_subdealerid = d.subdealerid AND pt.src_system = d.src_system AND pt.isdeleted = 0 AND pt.ddatee &lt; pt.ddateb
        JOIN src_system ss ON ss.id = pt.src_system
        JOIN agents a ON a.id = d.manager
        JOIN subdealer s ON s.src_id = pt.subdealerid AND s.src_system = pt.src_system
WHERE 
    (d.manager = ? OR 0 = ?) AND
    pt.ddateb &gt;= DATEADD(dd, -?, getdate())
ORDER BY
    a.name,
    ss.name,
    d.name,
    s.name,
    pt.terminalid</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!manager.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!manager.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!days.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
  <Code>Shared Function SetColumn(ByVal row As Integer, ByVal column As Integer) As String
  While row &gt; column
    If column &gt;= row Then Exit While
    row = row - column
  End While

  Return row
End Function

Shared Function SetRow(ByVal row As Integer, ByVal column As Integer) As String
  Dim i As Integer = 1

  While row &gt; column
    i = i + 1
    If column &gt;= row Then Exit While
    row = row - column
  End While

  Return i
End Function</Code>
