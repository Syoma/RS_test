  <DataSets>
    <DataSet Name="rc_unact">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select shortened, 
         if pic is null then 'Нет' else 'Есть' endif hasphoto 
  from person 
  where exists (select * 
                 from emp_contract ec1 
                 where ec1.person_id=person.person_id 
                    and datediff(day, ddateb, getdate()) &lt; 31 
                    
                    and not exists (select * 
                                    from emp_contract ec2 
                                    where ec1.person_id = ec2.person_id 
                                    and ec2.ddatee = ec1.ddateb - 1)) 
  order by 1</CommandText>
        <DataSourceName>rc_unact</DataSourceName>
      </Query>
      <Fields>
        <Field Name="shortened">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>shortened</DataField>
        </Field>
