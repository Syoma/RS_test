  <rd:ReportID>ca08a8b5-0a26-4c55-b60c-a52aaba69c13</rd:ReportID>
  <LeftMargin>2.5cm</LeftMargin>
  <DataSets>
    <DataSet Name="rc_unact">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select
       person.shortened,
       emp_leave.ddateb,
       emp_leave.ddatee,
       emp_leave.ddatee - emp_leave.ddateb + 1 as leavecount,
       emp_int_org.short_name org
   from person
     join emp_leave on emp_leave.person_id = person.person_id 
     join emp_contract on emp_contract.person_id = person.person_id
       and ( ( today() between emp_contract.ddateb and emp_contract.ddatee ) or
             ( emp_contract.ddateb &lt;= today() and emp_contract.ddatee is null)
            )         
     join emp_int_org on emp_int_org.int_org_id = emp_contract.int_org_id
   where emp_leave.ddateb between today() and today() + 9 and emp_int_org.int_org_id in (1, 2, 3, 4, 5)
   order by emp_leave.ddateb, person.shortened</CommandText>
        <DataSourceName>rc_unact</DataSourceName>
      </Query>
      <Fields>
        <Field Name="shortened">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>shortened</DataField>
        </Field>
        <Field Name="ddateb">
