  <DataSets>
    <DataSet Name="rc_unact">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT
       date(?) ddate1,
       date(?) ddate2,
       person.lname + ' ' + person.fname + ' ' + person.mname,
       emp_pos.name,
       (SELECT COUNT(*)
          FROM time_board
         WHERE time_board.person_id = person.person_id AND 
               time_board.ddate between ddate1 and ddate2 AND
               time_board.value = 'б') cnt_b,
       (SELECT COUNT(*)
          FROM time_board
         WHERE time_board.person_id = person.person_id AND 
               time_board.ddate between ddate1 and ddate2 AND
               time_board.value = 'а') cnt_a,
       (SELECT COUNT(*)
          FROM time_board
         WHERE time_board.person_id = person.person_id AND 
               time_board.ddate between ddate1 and ddate2 AND
               time_board.value = 'п') cnt_p
  FROM
       person
       join emp_rel on emp_rel.person_id = person.person_id and
                       emp_rel.ddateb = (SELECT MAX(ddateb) FROM emp_rel WHERE person_id = person.person_id and ddateb &lt; ddate2)
       join emp_dept on emp_dept.id = emp_rel.dept_id
       join emp_pos on emp_pos.id = emp_rel.pos_id
       join emp_dept_parent on emp_dept_parent.id = emp_dept.id and
                              emp_dept_parent.parent_id = ?
       join emp_user_dept on emp_user_dept.id = emp_dept.id and
                             emp_user_dept.iam = USER_ID(CURRENT USER)
ORDER BY 3</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter2.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter3.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>rc_unact_promt</DataSourceName>
      </Query>
      <Fields>
        <Field Name="ddate1">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>ddate1</DataField>
        </Field>
        <Field Name="ddate2">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>ddate2</DataField>
        </Field>
        <Field Name="person_lname_____person_fname_____person_mname">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>person.lname+' '+person.fname+' '+person.mname</DataField>
        </Field>
        <Field Name="name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>name</DataField>
        </Field>
        <Field Name="cnt_b">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>cnt_b</DataField>
        </Field>
        <Field Name="cnt_a">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>cnt_a</DataField>
        </Field>
        <Field Name="cnt_p">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>cnt_p</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ddw_dept">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT emp_dept.id, emp_dept.name
FROM emp_dept
       join emp_user_dept on emp_user_dept.id = emp_dept.id and
                             emp_user_dept.iam = USER_ID(CURRENT USER)
ORDER BY 2</CommandText>
        <DataSourceName>rc_unact_promt</DataSourceName>
      </Query>
      <Fields>
