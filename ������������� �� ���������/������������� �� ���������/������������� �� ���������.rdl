  <DataSets>
    <DataSet Name="main">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT
		partners_groups.name          manager,
		buyer_contract.ndoc           ndoc,
		partners.name + ' №' + buyer_contract.ndoc + ' от ' + CONVERT(varchar(15), buyer_contract.ddate, 104) name,
		buyer_contract.ddate          ddate,
		buyer_contract.ddatee         ddatee
FROM
		buyer_contract
			join contract_type on contract_type.id = buyer_contract.type
			join buyers on buyers.id = buyer_contract.client
			join partners on partners.id = buyers.partner
			join partners_groups_tree pgt on pgt.id = partners.parent
			join prefs on prefs.id = pgt.parent
			join partners_groups on prefs.id = partners_groups.id
WHERE
		    contract_type.code IN ('rent', 'rentterm', 'placement')
		and prefs.type = 1
		and concept = 1078
		and isnull(ddatee,'1900-01-01') &gt; today()
ORDER BY
		partners_groups.name,
		buyer_contract.ndoc desc</CommandText>
        <DataSourceName>main</DataSourceName>
      </Query>
      <Fields>
        <Field Name="manager">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>manager</DataField>
        </Field>
        <Field Name="ndoc">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>ndoc</DataField>
        </Field>
        <Field Name="name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>name</DataField>
        </Field>
        <Field Name="ddate">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>ddate</DataField>
        </Field>
        <Field Name="ddatee">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>ddatee</DataField>
        </Field>
      </Fields>
