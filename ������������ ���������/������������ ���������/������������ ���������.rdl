        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ord">
          <DataField>ord</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>cat</DataSourceName>
        <CommandText>select 
		0 id,
		'&lt;Агент не задан&gt;' name,
		0 ord
union
select
		agents.id,
		agents.name,
		1 ord
from
		agents
			join agent_groups_tree agt on agt.id = agents.a_group
where
		    isagent=1
		and issecurer=1
		and agt.parent = 1143
order by
		ord,
		name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
