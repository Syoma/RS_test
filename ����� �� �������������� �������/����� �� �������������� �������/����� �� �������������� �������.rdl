      </Fields>
      <Query>
        <DataSourceName>pr_cat_old</DataSourceName>
        <CommandText>BEGIN
	DECLARE @sql varchar(8000);
	DECLARE @fid int;
	SET @sql='
	SELECT
		ndoc,
		agent_name,
		buyer_name,
		date_create,
		date_delete,
		storage_name,
		goods_name,
		ddate,
		vol
	FROM
		rep_reserved_orders('+CAST(@site AS VARCHAR)+', '+CAST(@period_length AS VARCHAR)+')';
		
	SET @fid=(SELECT fid FROM site WHERE id=@site);
	
	exec get_data_filial @sql, @fid;
END</CommandText>
        <QueryParameters>
          <QueryParameter Name="@site">
            <Value>=Parameters!site.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@period_length">
            <Value>=Parameters!period_length.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="site">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>pr_cat_old</DataSourceName>
        <CommandText>SELECT
	id,
	name
FROM
	site
WHERE
	istarget=1
ORDER BY
	ord</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
