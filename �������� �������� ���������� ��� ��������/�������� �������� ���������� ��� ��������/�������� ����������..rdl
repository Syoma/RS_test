          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>create table #src (src int, descr nvarchar(100))
insert into #src (src, descr) values ('1', 'PPS (Terminal Monitoring Client)')
insert into #src (src, descr) values ('2', 'OSMP')
insert into #src (src, descr) values ('3', 'Comepay')
select src, descr from #src</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
