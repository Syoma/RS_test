        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>CREATE TABLE #rep_cr_term_res(res varchar(255)) 
CREATE TABLE #rep_cr_term_temp(terminalid int, code varchar(100), isdeleted int, src_system int)
INSERT INTO  #rep_cr_term_temp
    (terminalid,code,isdeleted,src_system)
SELECT   terminalid, code, isdeleted, src_system
      FROM [RC_UNACT].[rc_unact].[dbo].[pps_terminal]
      WHERE code=@code
 IF (SELECT   count(*) FROM #rep_cr_term_temp WHERE isdeleted='0')&gt;'0'
    BEGIN
      INSERT INTO  #rep_cr_term_res(res)
      SELECT   'Работающий терминал с префиксом'+' '+@code+', '+'найден - его номер - '+cast((SELECT  terminalid FROM #rep_cr_term_temp WHERE isdeleted='0') AS nvarchar(20))
     END
IF (SELECT   count(*) FROM #rep_cr_term_temp WHERE isdeleted='1'AND src_system=@src)&gt;'0'
     BEGIN
      INSERT INTO  #rep_cr_term_res(res) SELECT 'Найден удаленный терминал в этой же системе с префиксом'+' '+@code+' '+'его номер - '+cast((SELECT terminalid FROM #rep_cr_term_temp WHERE isdeleted='1'AND src_system=@src) AS nvarchar(20))
      END
 IF (SELECT   count(*) FROM #rep_cr_term_temp WHERE isdeleted='1'AND src_system&lt;&gt;@src)&gt;'0'
      BEGIN
   INSERT INTO  #rep_cr_term_res(res) SELECT 'Найден удаленный терминал в другой системе с префиксом'+' '+@code+' '+'его номер - '+cast((SELECT   terminalid FROM #rep_cr_term_temp WHERE isdeleted='1' AND src_system&lt;&gt;@src) AS nvarchar(20))
       END
 IF (SELECT   count(*) FROM #rep_cr_term_temp) ='0' BEGIN
   INSERT INTO  #rep_cr_term_res(res) SELECT 'Терминалов с этим префиксом не найдено'
       END
SELECT res FROM #rep_cr_term_res</CommandText>
        <QueryParameters>
          <QueryParameter Name="@code">
            <Value>=Parameters!code.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@src">
            <Value>=Parameters!src.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet1">
