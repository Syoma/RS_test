  <DataSets>
    <DataSet Name="rc_unact">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select bc.ndoc,(select partners.name from partners join buyers where buyers.id=bc.client) partner,cp.name, 
ct.ddateb contract_b,
ct.ddatee contract_e,
op.ddateb placement_b,
op.ddatee placement_e,
datediff(day,greater(op.ddateb,(select ddateb from period where period.id=?)),lesser(op.ddatee,(select ddatee from period where period.id=?)))
-datediff(day,greater(ct.ddateb,(select ddateb from period where period.id=?)),lesser(ct.ddatee,(select ddatee from period where period.id=?))) delta,
isnull((select status from contract_period where contract_period.period=? and contract_period.contract=bc.id),0) ispaid
from buyer_contract bc
join contract_placement cp
join (select cplacement, max(value) rate,min(ddateb) ddateb,min(ddatee) ddatee 
from contract_term group by cplacement) ct on ct.cplacement=cp.id
join (select cplacement,min(ddate) ddateb,max(if ddatee is null then '9999-01-01' else ddatee endif) ddatee
from osmp_placement group by cplacement) op on op.cplacement=cp.id
where datediff(day,greater(ct.ddateb,(select ddateb from period where period.id=?)),lesser(ct.ddatee,(select ddatee from period where period.id=?)))&lt;&gt;
                     datediff(day,greater(op.ddateb,(select ddateb from period where period.id=?)),lesser(op.ddatee,(select ddatee from period where period.id=?))) and
ct.ddateb&lt;=(select ddatee from period where period.id=?) and ct.ddatee&gt;=(select ddateb from period where period.id=?)
order by delta;</CommandText>
