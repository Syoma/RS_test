      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select id,DATEFORMAT( ddateb, 'mm/yy' ) per from period where ddatee&gt;(select min(ddateb) from contract_term) and ddatee&lt;dateadd(month,3,today()) order by ddateb desc</CommandText>
