  <DataSets>
    <DataSet Name="sellers">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact_old</DataSourceName>
        <CommandText>select
	sellers.id id,
	sellers.name name
from partners
	join partners_groups_tree pgt on pgt.id = partners.parent and pgt.parent in ( 36014, 4357 )
	join sellers on sellers.partner = partners.id
order by sellers.name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
