  <DataSets>
    <DataSet Name="rc_unact">
      <Fields>
        <Field Name="shot_ddate">
          <DataField>shot_ddate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="aso_state_name">
          <DataField>aso_state_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="aso_state_info">
          <DataField>aso_state_info</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="shot_okvalue">
          <DataField>shot_okvalue</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="shot_factvalue">
          <DataField>shot_factvalue</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="shot_reason">
          <DataField>shot_reason</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>rc_unact</DataSourceName>
        <CommandText>SELECT
	shot.ddate shot_ddate,
	aso_state.name aso_state_name,
	aso_state.info aso_state_info,
	shot.okvalue shot_okvalue,
	shot.factvalue shot_factvalue,
	shot.reason shot_reason
FROM
	aso_state
	JOIN shot ON aso_state.code = shot.code
WHERE
	shot.ddate between ? and ?
	AND
	aso_state.resp='Кулага'
	AND
	shot.isforgiven=0
	AND
	shot_okvalue&lt;shot.factvalue</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!ddateb.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!ddatee.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
  <Code>Shared Function SetColumn(ByVal row As Integer, ByVal column As Integer) As String
  While row &gt; column
    If column &gt;= row Then Exit While
    row = row - column
  End While

  Return row
End Function

Shared Function SetRow(ByVal row As Integer, ByVal column As Integer) As String
  Dim i As Integer = 1

  While row &gt; column
    i = i + 1
    If column &gt;= row Then Exit While
    row = row - column
  End While

  Return i
End Function</Code>
