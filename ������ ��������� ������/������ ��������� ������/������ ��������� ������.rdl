    <DataSet Name="tables">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT 
        table_name_value, 
        table_name_label
FROM
        (
            SELECT 
                    NULL table_name_value, 
                    '&lt;Все таблицы&gt;' table_name_label, 
                    0 ordnum
            UNION
            SELECT 
                    table_name, 
                    table_name, 
                    1
            FROM 
                    systab 
            WHERE 
                    table_type = 1
        ) AS t(table_name_value, table_name_label, ordnum) 
ORDER BY 
        ordnum, 
        table_name_label</CommandText>
        <DataSourceName>procedure</DataSourceName>
      </Query>
      <Fields>
        <Field Name="table_name_value">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>table_name_value</DataField>
        </Field>
        <Field Name="table_name_label">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>table_name_label</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="users">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT 
        name_value, 
        name_label
FROM
        (
            SELECT 
                    NULL name_value, 
                    '&lt;Все пользователи&gt;' name_label, 
                    0 ordnum
            UNION
            SELECT 
                    name,
                    name,
                    1
            FROM 
                    sysusers 
            WHERE 
                    suid &gt; 0
        ) AS t(name_value, name_label, ordnum) 
ORDER BY 
        ordnum, 
        name_label</CommandText>
        <DataSourceName>procedure</DataSourceName>
      </Query>
      <Fields>
        <Field Name="name_value">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>name_value</DataField>
        </Field>
        <Field Name="name_label">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>name_label</DataField>
        </Field>
      </Fields>
    </DataSet>
