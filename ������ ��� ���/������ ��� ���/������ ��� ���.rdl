      <Query>
        <DataSourceName>proxycat_old</DataSourceName>
        <CommandText>SELECT
	s.id,
	s.name
FROM
	site s
WHERE
	s.name IN (
		'Север',
		'Юг',
		'Коломна',
		'Электросталь',
		'Руза'
	)
ORDER BY
