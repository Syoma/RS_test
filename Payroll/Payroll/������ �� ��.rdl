    <DataSet Name="persons">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select p.person_id, p.shortened
from person p
where exists(
select 1 from emp_rel join emp_dept
on emp_rel.dept_id=emp_dept.id
join emp_user_dept 
on emp_dept.id=emp_user_dept.id
where (emp_rel.ddatee is null or emp_rel.ddatee &gt;= DATEADD(month,-2,today())) 
and iam = user_id(current user) 
and p.person_id = emp_rel.person_id 
)
order by p.shortened</CommandText>
        <DataSourceName>rc_unact_old2</DataSourceName>
      </Query>
      <Fields>
        <Field Name="person_id">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>person_id</DataField>
        </Field>
        <Field Name="shortened">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>shortened</DataField>
        </Field>
      </Fields>
    </DataSet>
