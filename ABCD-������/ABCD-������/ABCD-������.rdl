    <DataSet Name="сm">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ord">
          <DataField>ord</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>cat</DataSourceName>
        <CommandText>select
		id,
		name,
		1 ord
from
		catmanager
where
		uses_matrix = 1
union all
select
		0 id,
		'&lt;Все&gt;' name,
		0 ord
order by
		ord,
		name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ddate">
      <Fields>
        <Field Name="ddate">
          <DataField>ddate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>cat</DataSourceName>
        <CommandText>select distinct
		ddatee ddate, 
		dbo.get_month_name(month(p.ddatee), 1) + ' ' +  convert(char(4),year(p.ddatee)) + 'г.' name
from 
		period p
where
		TODAY()&gt;=DATEADD(day, 1, p.ddatee)
order by 
		ddatee desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="site">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>cat</DataSourceName>
        <CommandText>SELECT
		id,
		name
FROM
		site
WHERE
		istarget = 1
ORDER BY
		name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="lggroup">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ord">
          <DataField>ord</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>cat</DataSourceName>
        <CommandText>select 
		lggroup.id   id,
		lggroup.name name,
		1            ord
from
		lggroup
			join catmanager_ggroup cgg on cgg.lggroup = lggroup.id
where
		   cgg.catmanager = ?
		or 0 = ?
union all
select
		0       id,
		'&lt;Все&gt;' name,
		0       ord
order by 
		ord,
		name</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!cm.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!cm.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
