      <Query>
        <DataSourceName>proxycat</DataSourceName>
        <CommandText>exec dbo.rep_ABCD @lggroup, @cm, @site, @ddate</CommandText>
        <QueryParameters>
          <QueryParameter Name="@lggroup">
            <Value>=Parameters!lggroup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@cm">
