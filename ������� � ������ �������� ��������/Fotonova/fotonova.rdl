      <Query>
        <DataSourceName>unact_old</DataSourceName>
        <CommandText>select 
adress, g_name, box, volume  
from ask_fotonova_turnovered_remains(?, ?, ?, ?)</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter2.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Join (Parameters!storages.Value,",")</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Join (Parameters!goods_groups.Value,",")</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="storages">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>unact_old</DataSourceName>
        <CommandText>--все склады из группы Логистические услуги 
select
   id,
   name
FROM dbo.storages
where s_group in (select id from storages_groups_tree where parent = (SELECT id FROM dbo.storages_groups where name = 'Логистические услуги'))
ORDER  BY name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="goods_groups">
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>unact_old</DataSourceName>
        <CommandText>--Товары группы Логистические услуги
select
		goods_groups.id id,
		replicate('  ',2 * (goods_groups.tlev - (select gg.tlev from goods_groups gg where gg.id = 4087))) + goods_groups.name name
from
		goods_groups_tree ggt
			join goods_groups on goods_groups.id = ggt.id
where
		ggt.parent = 4087
order by
		goods_groups.sortcolumn</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
  <Code>Shared Function SetColumn(ByVal row As Integer, ByVal column As Integer) As String
  While row &gt; column
    If column &gt;= row Then Exit While
    row = row - column
  End While

  Return row
End Function

Shared Function SetRow(ByVal row As Integer, ByVal column As Integer) As String
  Dim i As Integer = 1

  While row &gt; column
    i = i + 1
    If column &gt;= row Then Exit While
    row = row - column
  End While

  Return i
End Function</Code>
