      <Query>
        <DataSourceName>rc_unact_old</DataSourceName>
        <CommandText>select m.id id, m.name name
from
(select id, name, name name_order from agents where position = 1272
union
select 0 id, convert(varchar(255), '&lt; Все менеджеры &gt;') name, convert(varchar(255), '###') name_order ) m
order by m.name_order asc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
