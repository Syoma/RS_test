    <DataSet Name="DataSource1">
      <Fields>
        <Field Name="author">
          <DataField>author</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Оформление__Анализ_проблемы">
          <DataField>Оформление-&gt;Анализ проблемы</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Переписка__Анализ_проблемы">
          <DataField>Переписка-&gt;Анализ проблемы</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Переписка__Ожидание_ответа">
          <DataField>Переписка, Ожидание ответа</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Задача_закрыта">
          <DataField>Задача закрыта</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT cg.author,
sum(case when cast(ci.oldstring as nvarchar(50))='Draft' and cast(ci.newstring as nvarchar(50))='Problem analysis' then 2 else 0 end) as [Оформление-&gt;Анализ проблемы],
sum(case when cast(ci.oldstring as nvarchar(50))='Переписка' and cast(ci.newstring as nvarchar(50))='Problem analysis' then 2 else 0 end) as [Переписка-&gt;Анализ проблемы],
sum(case when cast(ci.oldstring as nvarchar(50))='Переписка' and cast(ci.newstring as nvarchar(50))='Переписка' then 2 else 0 end) as [Переписка, Ожидание ответа],
sum(case when cast(ci.newstring as nvarchar(50))='Closed' then 10 else 0 end) as [Задача закрыта]
from [JIRA].[jira].[jiraissue] j
left join [JIRA].[jira].[changegroup] cg on cg.issueid=j.ID
left join [JIRA].[jira].[changeitem]ci on cg.ID=ci.groupid
where j.project='10311' and j.issuetype='52' and ci.FIELD='status'
group by cg.AUTHOR
having sum(case when cast(ci.oldstring as nvarchar(50))='Draft' and cast(ci.newstring as nvarchar(50))='Problem analysis' then 2 else 0 end)&gt;'0' or
sum(case when cast(ci.oldstring as nvarchar(50))='Переписка' and cast(ci.newstring as nvarchar(50))='Problem analysis' then 2 else 0 end)&gt;'0' or
sum(case when cast(ci.oldstring as nvarchar(50))='Переписка' and cast(ci.newstring as nvarchar(50))='Переписка' then 2 else 0 end)&gt;'0' or
sum(case when cast(ci.newstring as nvarchar(50))='Closed' then 10 else 0 end)&gt;'0'
order by cg.AUTHOR</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
