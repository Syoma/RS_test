  <DataSets>
    <DataSet Name="rc_unact">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT
       storages.name,
       goods.short_name,
       remains.ddate,
       SUM(remains.volume/ms_rel_sets.rel)
  FROM
       remains
       join storages on remains.storage = storages.id
       join (extra
             join etype on extra.etype = etype.id and etype.code='storagetransit'
            ) on extra.record_id = storages.id
       join goods on remains.goods = goods.id
       join my_ggroup on my_ggroup.id = goods.g_group and my_ggroup.iam = USER_ID(CURRENT USER)
       join ms_rel_sets on ms_rel_sets.ms_id = my_ggroup.vmeas and
                           ms_rel_sets.msrh_id = goods.msrh_id
GROUP BY storages.name, goods.short_name, remains.ddate
ORDER BY storages.name, goods.short_name
;</CommandText>
        <DataSourceName>rc_unact</DataSourceName>
      </Query>
      <Fields>
        <Field Name="name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>name</DataField>
        </Field>
        <Field Name="short_name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>short_name</DataField>
        </Field>
        <Field Name="ddate">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>ddate</DataField>
        </Field>
        <Field Name="SUM_remains_volume_ms_rel_sets_rel_">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>SUM(remains.volume/ms_rel_sets.rel)</DataField>
        </Field>
      </Fields>
    </DataSet>
